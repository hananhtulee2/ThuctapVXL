.................... usi8 nd_tren = 34;                                                       
.................... usi8 nd_duoi = 32;   
.................... #define ttqn tttd  
....................  
.................... int1 tttd=0,buz_ena=0,ttmd=0;           
.................... void so_sanh_1nd(usi16 nd1)                         
.................... {                                    
....................    if((nd1>nd_tren)&&(tttd==0)) 
....................    {                      
....................          tttd = 1;   ttmd = 0;                                                            
....................          if(buz_ena) buzzer_on();                                    
....................          triac_2_off();  
....................    }                                                                                     
....................    else if((nd1<nd_tren)&&(tttd==1))            
....................    {                        
....................          tttd = 0;   
....................          if(buz_ena) buzzer_off();            
....................    }                                 
....................    else if((nd1<nd_duoi)&&(tttd==0))  
....................    { 
....................       if(ttmd==0)  
....................       { 
....................          triac_2_on(); ttmd=1; 
....................          buzzer_on_off(300); 
....................       } 
....................    }  
.................... }      
....................  
....................  
.................... void so_sanh_and_2nd(usi16 nd1, usi16 nd2)                         
.................... {                                    
....................    if((nd1>nd_tren)&&(nd2>nd_tren)&&(tttd==0)) 
....................    {                      
....................       tttd = 1;    ttmd = 0;                                                            
....................       if(buz_ena) buzzer_on();                                    
....................       triac_2_off();  
....................    }                                                                                     
....................    else if((nd1<nd_tren)&&(nd2<nd_tren)&&(tttd==1))            
....................    {                        
....................       tttd = 0;   
....................       if(buz_ena) buzzer_off();                       
....................    }                                 
....................    else if((nd1<nd_duoi)&&(nd2<nd_duoi)&&(tttd==0))   
....................    { 
....................       if(ttmd==0)  
....................       { 
....................          triac_2_on(); ttmd=1; 
....................          buzzer_on_off(300); 
....................       } 
....................    }  
.................... }      
....................  
.................... void so_sanh_or_2nd(usi16 nd1, usi16 nd2)                         
.................... {                                    
....................    if(((nd1>nd_tren)||(nd2>nd_tren))&&(tttd==0)) 
....................    {                      
....................          tttd = 1;  ttmd = 0;                                                             
....................          if(buz_ena) buzzer_on();                                    
....................          triac_2_off();  
....................    }                                                                                     
....................    else if(((nd1<nd_tren)&&(nd2<nd_tren))&&(tttd==1))            
....................    {                        
....................          tttd = 0;                       
....................          if(buz_ena) buzzer_off();            
....................    }                                
....................    else if(((nd1<nd_duoi)||(nd2<nd_duoi))&&(tttd==0))     
....................    { 
....................       if(ttmd==0)  
....................       { 
....................          triac_2_on(); ttmd=1; 
....................          buzzer_on_off(300); 
....................       } 
....................    }  
....................    
.................... }                                    
....................                                     
.................... void so_sanh_nd_dk_buzzer(usi16 nd1, usi16 nd2, int8 sel)                         
.................... {                                                  
....................    if(sel==1)      so_sanh_1nd(nd1); 
....................    else if(sel==2) so_sanh_and_2nd(nd1,nd2); 
....................    else if(sel==3) so_sanh_or_2nd(nd1,nd2);    
.................... } 
