.................... void ds18b20_khoi_tao() 
.................... {                          
....................    touch_present(); 
....................    touch_write_byte(skip_rom);       
....................    touch_write_byte(write_scratchpad);                      
....................    touch_write_byte(0x0);        
....................    touch_write_byte(0x0);     
....................    touch_write_byte(0x7f); //12 bit 
....................  
....................    touch_present();    
....................    touch_write_byte(skip_rom); 
....................    touch_write_byte(convert_t);      
.................... } 
