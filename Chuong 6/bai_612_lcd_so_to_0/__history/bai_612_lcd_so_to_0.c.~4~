#include <tv_kit_vdk_pic_all.c>    
void main() {
   set_up_port();
   lcd_setup() ; 
   
   lcd_goto_xy(0,0);
   lcd_data("Hien thi so 0 o hang 2");                      
   
   lcd_khoi_tao_cgram_so_to();
   lcd_hien_thi_so_to(0,2,0);                   
   lcd_hien_thi_so_to(1,2,4);
   lcd_hien_thi_so_to(2,2,7);
   lcd_hien_thi_so_to(3,2,10);    
   lcd_hien_thi_so_to(4,2,13);
   lcd_hien_thi_so_to(5,2,16);
   while(true);

}
