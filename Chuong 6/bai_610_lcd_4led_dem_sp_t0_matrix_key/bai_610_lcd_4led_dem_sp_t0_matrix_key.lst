.................... usi16 so_ng,so_tp; 
.................... void float_to_ng_2so_tp(float st)                       
.................... {    
....................    so_ng = st/1;       //nguyen                           
....................    st = st-so_ng;      //tphan 
....................    st = st*100;                                                          
....................    so_tp = st/1;    
.................... }  
