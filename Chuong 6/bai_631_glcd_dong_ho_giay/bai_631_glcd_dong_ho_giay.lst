.................... void gdram_vdk_to_gdram_glcd_area(int8 x, y, rong, cao) 
.................... {                                                  
....................   int8 hesox,hesoy;  
....................   if (gdram_vdk.refresh)  
....................   {        
....................       if(((rong+x)/16)!=0) rong=((rong)/16)+1; 
