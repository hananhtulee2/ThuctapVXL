.................... #define  read_scratchpad  0xbe  
.................... #define  write_scratchpad 0x4e                                
.................... #define  copy_scratchpad  0x48   
.................... #define  convert_t        0x44  
.................... //xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx 
