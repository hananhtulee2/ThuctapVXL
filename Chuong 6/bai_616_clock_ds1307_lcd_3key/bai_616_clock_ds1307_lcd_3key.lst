....................       lcd_goto_xy(x,y);       
....................       if (*str == '\n')  
....................       {                    
....................          x++; 
....................          lcd_goto_xy(x,y); 
....................       }  
....................       else if (*str == '\r')  
....................       { 
....................          lcd_goto_xy(0,y); 
....................       }  
....................       else  
....................          { 
....................             lcd_data(*str); 
....................             y++;                       
....................              
....................          } 
....................       str++; 
....................    } 
.................... } 
