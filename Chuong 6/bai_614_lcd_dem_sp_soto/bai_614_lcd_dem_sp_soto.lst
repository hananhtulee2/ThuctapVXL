.................... unsigned int key_nhan()                
.................... {      
....................    unsigned int8 maphim,hang,cot;       
....................    maphim=hang=0xff; 
....................    for(cot=0;cot<4;cot++)                     
....................    {    
....................       output_b(maquet[cot]); 
....................       if     (!input(pin_b2)) {hang=3; break;} 
....................       else if(!input(pin_b3)) {hang=2; break;}        
....................       else if(!input(pin_b4)) {hang=1; break;} 
....................       else if(!input(pin_b5)) {hang=0; break;} 
....................    }           
