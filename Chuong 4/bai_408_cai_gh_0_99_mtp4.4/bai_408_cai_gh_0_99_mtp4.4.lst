.................... void xuat_2byte(unsigned int16 v) 
.................... { 
....................    int8 i; 
....................    #bit msb = v.15 
....................    for(i=0;i<16;i++) 
....................    {                                                  
....................       output_bit(ssdo,msb);     
....................       output_low(ssck);    
....................       output_high(ssck); 
....................       v= v<<1;          
....................    } 
.................... } 
