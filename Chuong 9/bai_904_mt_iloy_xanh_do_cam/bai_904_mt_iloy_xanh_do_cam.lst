.................... unsigned int16 ds18b20_read_temp()                                         
.................... {                                     
....................    unsigned int8  bl,bh;                                                              
....................    unsigned int16 w;  
....................    touch_write_byte(skip_rom); 
....................    touch_write_byte(read_scratchpad);                  
....................    bl = touch_read_byte(); 
....................    bh = touch_read_byte();                              
....................    w  = make16(bh,bl);                                                           
....................                                                          
....................    touch_present();    
....................    touch_write_byte(skip_rom); 
....................    touch_write_byte(convert_t);  
....................    return w; 
.................... }   
