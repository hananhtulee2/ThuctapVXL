.................... int1 dht11_check_response()         
.................... {                                 
....................    unsigned int8 cnt=0;    
....................    int1 tt=1, tt1=1; 
....................    do 
....................    { 
....................       if(dht11_pin_read()==0) cnt++; //       
....................       else                    tt=0;  //het o muc 0,xung ln 1,        
....................    }while ((cnt<200)&&(tt==1));      //dung: cnt<200 v tt=1; sai  
....................                                                 
....................    cnt=0; tt1=1; 
....................    do 
....................    { 
....................       if(dht11_pin_read()==1) cnt++;        
....................       else                    tt1=0;  //het o muc 1, xung xuong muc 0        
....................    }while ((cnt<200)&&(tt1==1));               
....................    return tt;  
.................... }        
