....................       tttd = 0;   
....................       if(buz_ena) buzzer_off();                       
....................    }                                 
....................    else if((nd1<nd_duoi)&&(nd2<nd_duoi)&&(tttd==0))   
....................    { 
....................       if(ttmd==0)  
....................       { 
....................          triac_2_on(); ttmd=1; 
....................          buzzer_on_off(300); 
....................       } 
....................    }  
.................... }      
....................  
.................... void so_sanh_or_2nd(usi16 nd1, usi16 nd2)                         
.................... {                                    
....................    if(((nd1>nd_tren)||(nd2>nd_tren))&&(tttd==0)) 
....................    {                      
....................          tttd = 1;  ttmd = 0;                                                             
....................          if(buz_ena) buzzer_on();                                    
....................          triac_2_off();  
....................    }                                                                                     
....................    else if(((nd1<nd_tren)&&(nd2<nd_tren))&&(tttd==1))            
....................    {                        
....................          tttd = 0;                       
....................          if(buz_ena) buzzer_off();            
....................    }                                
....................    else if(((nd1<nd_duoi)||(nd2<nd_duoi))&&(tttd==0))     
....................    { 
....................       if(ttmd==0)  
....................       { 
....................          triac_2_on(); ttmd=1; 
....................          buzzer_on_off(300); 
....................       } 
....................    }  
....................    
.................... }                                    
