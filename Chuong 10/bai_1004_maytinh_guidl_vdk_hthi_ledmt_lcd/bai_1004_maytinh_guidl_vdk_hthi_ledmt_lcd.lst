.................... void xuat_matranled( usi16 mahang,usi16 macot1, 
....................                      usi16 macot2,usi16 macot3) 
.................... {       
....................    xuat_2byte(macot3);   
*
0DB6:  MOVFF  2CE,2D0
0DBA:  MOVFF  2CD,2CF
0DBE:  RCALL  0D68
....................    xuat_2byte(macot2); 
0DC0:  MOVFF  2CC,2D0
0DC4:  MOVFF  2CB,2CF
0DC8:  RCALL  0D68
