....................       lcd_hien_thi_so_to(ng,x,y+3);   
....................       lcd_hien_thi_so_to(tr,x,y+6); 
....................       lcd_hien_thi_so_to(ch,x,y+9);  
....................       lcd_hien_thi_so_to(dv,x,y+12); 
....................       if((cn==0)&&(xvn)) 
....................       {      
....................          lcd_xoa_so_to(x,y); 
....................          if(ng==0)  
....................          { 
....................             lcd_xoa_so_to(x,y+3); 
....................             if(tr==0)  
....................             { 
....................                lcd_xoa_so_to(x,y+6); 
....................                if(ch==0) lcd_xoa_so_to(x,y+9); 
....................             } 
....................          } 
....................       } 
.................... } 
