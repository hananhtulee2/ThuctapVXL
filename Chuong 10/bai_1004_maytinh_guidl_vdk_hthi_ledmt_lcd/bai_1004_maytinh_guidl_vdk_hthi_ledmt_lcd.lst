.................... void xuat_2byte(unsigned int16 v) 
.................... { 
....................    int8 i; 
....................    #bit msb = v.15 
....................    for(i=0;i<16;i++) 
*
0D68:  MOVLB  2
0D6A:  CLRF   xD1
0D6C:  MOVF   xD1,W
0D6E:  SUBLW  0F
0D70:  BNC   0D90
....................    {                                                  
....................       output_bit(ssdo,msb);     
0D72:  BTFSC  xD0.7
0D74:  BRA    0D7A
0D76:  BCF    F8D.2
0D78:  BRA    0D7C
0D7A:  BSF    F8D.2
0D7C:  BCF    F96.2
....................       output_low(ssck);    
0D7E:  BCF    F96.0
0D80:  BCF    F8D.0
....................       output_high(ssck); 
0D82:  BCF    F96.0
0D84:  BSF    F8D.0
....................       v= v<<1;          
0D86:  BCF    FD8.0
0D88:  RLCF   xCF,F
0D8A:  RLCF   xD0,F
0D8C:  INCF   xD1,F
0D8E:  BRA    0D6C
....................    } 
0D90:  MOVLB  0
0D92:  RETURN 0
.................... } 
