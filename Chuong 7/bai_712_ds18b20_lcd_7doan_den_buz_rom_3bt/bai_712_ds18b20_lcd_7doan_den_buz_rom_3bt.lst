.................... int1 dht11_check_response()         
.................... {                                 
....................    unsigned int8 cnt=0;    
....................    int1 tt=1, tt1=1; 
....................    do 
....................    { 
....................       if(dht11_pin_read()==0) cnt++; //       
....................       else                    tt=0;  //het o muc 0,xung ln 1,        
....................    }while ((cnt<200)&&(tt==1));      //dung: cnt<200 v tt=1; sai  
....................                                                 
....................    cnt=0; tt1=1; 
....................    do 
....................    { 
....................       if(dht11_pin_read()==1) cnt++;        
....................       else                    tt1=0;  //het o muc 1, xung xuong muc 0        
....................    }while ((cnt<200)&&(tt1==1));               
....................    return tt;  
.................... }        
.................... //xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx 
.................... //ham doc 5 byte cua DHT11 
.................... //xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx   
.................... unsigned int8 dht11_read()         
.................... {                                                                                
....................    unsigned int8 kq,j;     
....................    for(j=0;j<8;j++)                                                
....................    {                                                          
....................       while(!dht11_pin_read());  //doi het muc 0     
....................       delay_us(30); 
....................       if(dht11_pin_read()==1)  
....................       {              
....................          kq = (kq<<1)+1;     
....................          delay_us(40);           //bit 1 thi them delay 
....................       }                           
....................       else kq = kq<<1;   
....................    }     
....................    return kq;  
.................... }   
