....................       else if (*str == '\r')  
....................       { 
....................          lcd_goto_xy(0,y); 
....................       }  
....................       else  
....................          { 
....................             lcd_data(*str); 
....................             y++;                       
....................              
....................          } 
....................       str++; 
....................    } 
